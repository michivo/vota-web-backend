{
  "name": "vota-web-backend",
  "version": "1.0.0",
  "description": "Backend for VOTA WEB",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "start": "node dist/src/main.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/main.ts",
    "lint": "eslint . --ext .ts",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copyfiles",
    "copyfiles": "copyfiles src/features/**/*.json dist/ && copyfiles src/keys/*.json dist/",
    "maketypes": "cd src/typings && typeconv -f ts -t oapi *.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michivo/vota-web-backend.git"
  },
  "keywords": [
    "VOTA",
    "Voting"
  ],
  "author": "Michael Faschinger <michael@daredevels.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michivo/vota-web-backend/issues"
  },
  "homepage": "https://github.com/michivo/vota-web-backend#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6"
  }
}
